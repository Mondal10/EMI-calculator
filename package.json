{
  "name": "uidev-project-template",
  "version": "1.0.0",
  "description": "Project Template for UI developer coding exercise.",
  "author": "Medly Pharmacy",
  "keywords": [
    "Medly",
    "UI Challenge"
  ],
  "license": "ISC",
  "private": true,
  "scripts": {
    "start": "webpack-dev-server --open --mode development --hot",
    "build": "webpack --mode production",
    "test": "run-p test:jest",
    "test:jest": "jest --config config/jest/jest.config.js",
    "test:coverage": "run-p \"test:jest --coverage\"",
    "test:update": "run-p \"test:jest -- -u\"",
    "test:watch": "run-p \"test:jest -- --watch\"",
    "pretty": "prettier --check 'src/**/*.js'",
    "lint": "run-p -c lint:**",
    "lint:js": "eslint src",
    "lint:styles": "stylelint 'src/**/*.scss'"
  },
  "devDependencies": {
    "@babel/core": "^7.8.7",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/polyfill": "^7.8.7",
    "@babel/preset-env": "^7.8.7",
    "@babel/preset-react": "^7.8.3",
    "@stryker-mutator/core": "^2.5.0",
    "@testing-library/dom": "^6.15.0",
    "@testing-library/jest-dom": "^5.1.1",
    "@testing-library/react": "^9.5.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.0.6",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^3.4.2",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.11.2",
    "enzyme-to-json": "^3.4.4",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.18.3",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "image-webpack-loader": "^6.0.0",
    "jest": "^25.1.0",
    "jest-styled-components": "^7.0.0",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.13.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1",
    "react-test-renderer": "^16.13.0",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.1.3",
    "stylelint": "^13.2.0",
    "stylelint-config-recommended": "^3.0.0",
    "stylelint-processor-styled-components": "^1.10.0",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3"
  },
  "dependencies": {
    "chart.js": "^2.9.3",
    "react": "^16.13.0",
    "react-chartjs-2": "^2.10.0",
    "react-dom": "^16.13.0",
    "styled-components": "^5.0.1"
  }
}
